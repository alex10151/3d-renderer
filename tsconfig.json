{
    "compilerOptions": {
        "jsx": "react",
        "module": "ESNext",
        "noImplicitAny": true,
        "outDir": "./build/",
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "es5",
        "allowSyntheticDefaultImports": true
    },
    "resolve": {
        "extensions": [
            ".tsx",
            ".js",
            ".jsx",
            ".ts",
        ],
    },
}